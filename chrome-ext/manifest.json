{
  "manifest_version": 2,

  "name": "Getting started example",
  "description": "This extension shows a Google Image search result for the current page",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs",
    "\u003Call_urls\u003E",
    "cookies",
    "contextMenus",
    "unlimitedStorage",
    "notifications",
    "storage",
    "clipboardWrite",
    "activeTab",
    "cookies",
    "http://*/*",
    "https://*/*",
    "<all_urls>"
  ]
}
